@model IEnumerable<UserListEdit>
@{}
@if (Model != null && Model.Count() > 0)
{
    @foreach (var i in Model)
    {
        <div class="max-w-xs sm:max-w-sm md:max-w-md lg:max-w-lg bg-white border border-gray-200 rounded-lg shadow-sm dark:bg-gray-800 dark:border-gray-700">
            <div class="flex flex-col items-center p-8">
                <img class="w-24 h-24 mb-3 rounded-full shadow-lg" src="~/UserProfile/@i.UserProfile" alt="@i.UserName" />
                <h5 class="mb-1 text-xl font-medium text-gray-900 dark:text-white">@i.UserName</h5>
                <span class="text-sm text-gray-500 dark:text-gray-400">@i.UserAddress</span>
                <div class="flex mt-4 md:mt-6 gap-2">
                    <a asp-action="Details" asp-route-id="@i.EncId" class="text-white bg-blue-700 hover:bg-blue-800 py-3 px-5 rounded-lg">Details</a>
                    <a asp-action="Edit" asp-route-id="@i.EncId" class="text-white bg-green-700 hover:bg-blue-800 py-3 px-5 rounded-lg">Edit</a>
                    <a asp-action="Delete" asp-controller="Home" asp-route-id="@i.UserId" data-ajax-method="get" data-ajax="true" data-ajax-confirm="Are u sure you want to delete this?" data-ajax-complete="DeleteUser" class="text-white bg-red-700 hover\:bg-red-800 rounded-lg px-5 py-3">Delete</a>
                </div>
            </div>
        </div>
    }
}
else
{
    <div class="text-gray-600" style="margin-top:200px;">
        <p>Data not found</p>
    </div>
}